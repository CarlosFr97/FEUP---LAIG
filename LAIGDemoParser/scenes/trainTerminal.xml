<SCENE>
  <!-- Inicial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
  </INITIALS>

  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
  </ILLUMINATION>

  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light0">
      <enable value ="1" />
      <position  x="0" y="12.5" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light1">
      <enable value ="1" />
      <position x="30" y="12.5" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light2">
      <enable value ="1" />
      <position x="0" y="12.5" z="20" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
      <enable value ="1" />
      <position x="30" y="12.5" z="20" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light4">
      <enable value ="1" />
      <position x="0" y="12.5" z="40" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light5">
      <enable value ="1" />
      <position x="30" y="12.5" z="40" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light6">
      <enable value ="1" />
      <position x="0" y="12.5" z="60" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light7">
      <enable value ="1" />
      <position x="30" y="12.5" z="60" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>
  </LIGHTS>

  <TEXTURES>
    <!-- Tree trunk -->
    <TEXTURE id="trunk">
      <file path="images/trunk-image.jpg" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <!-- Tree leaves -->
    <TEXTURE id="folhas">
      <file path="images/leaves.jpg" />
      <amplif_factor s="3" t="4" />
    </TEXTURE>

    <!-- Rocks -->
    <TEXTURE id="rocks">
      <file path="images/rocks.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Glass work -->
    <TEXTURE id="vidral">
      <file path="images/vidral.jpg" />
      <amplif_factor s="15" t="10" />
    </TEXTURE>

    <!-- Street bench -->
    <TEXTURE id="banco">
      <file path="images/bank.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- gravilha para chão comboio -->
    <TEXTURE id="gravilha">
      <file path="images/gravilha.jpg" />
      <amplif_factor s="0.25" t="0.25" />
    </TEXTURE>
  </TEXTURES>

  <MATERIALS>
    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="separatorMaterial">
      <shininess value="1"/>
      <specular r="0.1" g="0.1" b="0.1" a="1"/>
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="carriageMaterial">
      <shininess value="1"/>
      <specular r="0.807" g="0.807" b="0.0" a="1"/>
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.1" g="0.1" b="0.1" a="1" />
    </MATERIAL>

    <MATERIAL id="wheelMaterial">
      <shininess value="1"/>
      <specular r="0.268" g="0.274" b="0.294" a="1"/>
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.1" g="0.1" b="0.1" a="1" />

    </MATERIAL>

  </MATERIALS>

  <LEAVES>
  </LEAVES>

  <NODES>
    <ROOT id="root"/>

    <!-- raiz -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="chaoCarris"/>
        <NODEREF id="laterias"/>
        <NODEREF id="paredes"/>
        <NODEREF id="rebordoPorta"/>
        <NODEREF id="train"/>

      </DESCENDANTS>
    </NODE>

    <!-- chão onde passam os carris -->
    <NODE id="chaoCarris">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="gravilha"/>

      <SCALE sx="35" sy="0" sz="40" />
      <TRANSLATION x="0" y="0" z="1.5" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>
    </NODE>

    <!-- cais laterais -->
    <NODE id="laterias">
      <!-- definir materiais e texturas aqui-->
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="lateralEsquerda"/>
        <NODEREF id="lateralDireita"/>
      </DESCENDANTS>
    </NODE>

    <!-- cais do lado esquerdo -->
    <NODE id="lateralEsquerda">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="70" sy="5" sz="15" />

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- cais do lado direito -->
    <NODE id="lateralDireita">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="70" sy="5" sz="15" />
      <TRANSLATION x="0" y="0" z="2.5"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- paredes -->
    <NODE id="paredes">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="paredeEsquerda"/>
        <NODEREF id="paredeDireita"/>
      </DESCENDANTS>
    </NODE>

    <!-- parede esquerda -->
    <NODE id="paredeEsquerda">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="parte1"/>
        <NODEREF id="parte2"/>
      </DESCENDANTS>
    </NODE>

    <!-- parede direita -->
    <NODE id="paredeDireita">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="12.5" y="0.35" z="60"/>

      <DESCENDANTS>
        <NODEREF id="parte1"/>
        <NODEREF id="parte2"/>
      </DESCENDANTS>
    </NODE>

    <!-- 1ºparte da parede -->
    <NODE id="parte1">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="15" sy="14" sz="0.5" />
      <TRANSLATION x="0" y="0.15" z="0.5"/>
      <ROTATION axis="y" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- 2ºparte da parede -->
    <NODE id="parte2">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="15" sy="14" sz="0.5" />
      <TRANSLATION x="0" y="0.3" z="0.5"/>
      <ROTATION axis="y" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="floor"/>
    
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- redordo da porta -->
    <NODE id="rebordoPorta">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>
      <SCALE sx="6" sy="1.05" sz="5" />
      <TRANSLATION x="0" y="0" z="4" />

      <DESCENDANTS>
        <NODEREF id="rebordoEsquerda"/>
        <NODEREF id="rebordoDireita"/>
        <NODEREF id="rebordoCima"/>
      </DESCENDANTS>
    </NODE>

    <!-- rebordo da porta, parte do lado esquerdo -->
    <NODE id="rebordoEsquerda">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="0.25" sy="14" sz="1" />
      <TRANSLATION x="0" y="0.15" z="0.5"/>
      <ROTATION axis="y" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- rebordo da porta, parte do lado direito -->
    <NODE id="rebordoDireita">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="0.25" sy="14" sz="1" />
      <TRANSLATION x="15" y="0.15" z="0.5"/>
      <ROTATION axis="y" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- rebordo da porta, parte de cima -->
    <NODE id="rebordoCima">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="8.25" sy="0.25" sz="1" />
      <TRANSLATION x="0" y="37" z="0"/>
      <ROTATION axis="x" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- rectangulo genrico -->
    <NODE id="myRectangle">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="0.5" sy="0.5" sz="1.5" />
      <TRANSLATION x="0" y="0" z="-0.5" />

      <DESCENDANTS>
        <NODEREF id="faceCubo1" />
        <NODEREF id="faceCubo2" />
        <NODEREF id="faceCubo3" />
        <NODEREF id="faceCubo4" />
        <NODEREF id="faceCubo5" />
        <NODEREF id="faceCubo6" />
      </DESCENDANTS>
    </NODE>

    <!-- A cube face -->
    <NODE id="faceCubo1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="1.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 1 1 0" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0.5" />
		  <ROTATION axis="y" angle="-90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 1 1 0" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo3">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="1" y="0" z="1.5" />
		  <ROTATION axis="y" angle="90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 1 1 0" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo4">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="1" y="0" z="0.5" />
      <ROTATION axis="y" angle="180" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 1 1 0" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="faceCubo5">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="-90" />
      <TRANSLATION x="0" y="-1.5" z="1" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 1 1 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="train">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>
      <TRANSLATION x="18" y="-3" z="23"/>
      
      <DESCENDANTS>
        <NODEREF id="carriage_1"/>
        <NODEREF id="carriage_2"/>
        <NODEREF id="carriage_3"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="carriage_1">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="carriage"/>
        <NODEREF id="separator"/>
        <NODEREF id="top-light"/>
        <NODEREF id="left-light"/>
        <NODEREF id="right-light"/>
        <NODEREF id="front-window"/>
      </DESCENDANTS>

    </NODE>

     <NODE id="carriage_2">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="-13" y="0" z="0" />
      <DESCENDANTS>
        <NODEREF id="carriage"/>
        <NODEREF id="separator"/>
      </DESCENDANTS>

      </NODE>

     <NODE id="carriage_3">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>
      
      <TRANSLATION x="-26" y="0" z="0"/>
      <DESCENDANTS>
        <NODEREF id="carriage"/>
      </DESCENDANTS>

      </NODE>

    <NODE id="carriage">
      <MATERIAL id="carriageMaterial"/>
      <TEXTURE id="null"/>


      <SCALE sx="2.5" sy="1" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="left-side"/>
        <NODEREF id="right-side"/>
        <NODEREF id="top-side"/>
        <NODEREF id="bottom-side"/>
        <NODEREF id="back-side"/>
        <NODEREF id="front-side"/>

        <NODEREF id="left-wheel1"/>
        <NODEREF id="left-wheel2"/>
        <NODEREF id="left-wheel3"/>
        <NODEREF id="right-wheel1"/>
        <NODEREF id="right-wheel2"/>
        <NODEREF id="right-wheel3"/>

        <NODEREF id="left-window"/>
        <NODEREF id="right-window"/>

        <NODEREF id="left-door"/>
        <NODEREF id="right-door"/>


        <NODEREF id="smooth-roof"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="left-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="2.5" y="4" z="7.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="right-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="7.5" y="4" z="0.5" />
      <ROTATION axis="y" angle="180" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="top-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="-90" />
      <TRANSLATION x="2.5" y="-7.5" z="11" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="bottom-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="2.5" y="4" z="0.5" />
      <ROTATION axis="x" angle="90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="front-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="7.5" y="4" z="7.5" />
		  <ROTATION axis="y" angle="90" />
      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 7 0 "/>
      </DESCENDANTS>
    </NODE>

    <NODE id="back-side">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="2.5" y="4" z="0.5" />
		  <ROTATION axis="y" angle="-90" />
      

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 7 7 0"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="separator">
      <MATERIAL id="separatorMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="5.2" y="0.49" z="0.5"/>
      <SCALE sx="0.15" sy="0.95" sz="0.85"/>

      <DESCENDANTS>
        <NODEREF id="left-side"/>
        <NODEREF id="top-side"/>
        <NODEREF id="right-side"/>
        <NODEREF id="bottom-side"/>

      </DESCENDANTS>
    </NODE>

    <NODE id="top-light">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

       <TRANSLATION x="18.5" y="10" z="4"/> 
       <ROTATION axis="y" angle="90"/>
       <SCALE sx="1" sy="0.5" sz="0.5"/>

      <DESCENDANTS>
        <LEAF type="cylinder" args="1 1 1 20 20 1 1"/> 
      </DESCENDANTS>
    </NODE>

    <NODE id="left-wheel1">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>
      <TRANSLATION x="0" y="0" z="6.5"/>
      <DESCENDANTS>
        <NODEREF id="wheel"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="left-wheel2">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>
      <TRANSLATION x="-1" y="0" z="6.5"/>
      <DESCENDANTS>
        <NODEREF id="wheel"/>
      </DESCENDANTS>
    </NODE>

     <NODE id="left-wheel3">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="-1.75" y="0" z="6.5"/>
      <DESCENDANTS>
        <NODEREF id="wheel"/>
      </DESCENDANTS>
    </NODE>

     <NODE id="right-wheel1">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      
      <DESCENDANTS>
        <NODEREF id="wheel"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="right-wheel2">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="-0.75" y="0" z="0"/>
      <DESCENDANTS>
        <NODEREF id="wheel"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="right-wheel3">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      
      
      
      <TRANSLATION x="-1.75" y="0" z="0"/>
      
      
      <DESCENDANTS>
        <NODEREF id="wheel"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="wheel">
      <MATERIAL id="wheelMaterial"/>
      <TEXTURE id="null"/>

      
      <TRANSLATION x="6.5" y="4" z="0.75" />
      <SCALE sx="0.35" sy="1" sz="0.25"/>
      <ROTATION axis="y" angle="90"/>
      
      

      <DESCENDANTS>
        <LEAF type="sphere" args="1 20 20"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="smooth-roof">
      <MATERIAL id="carriageMaterial"/>
      <TEXTURE id="null"/>
      
      <TRANSLATION x="5" y="9.5" z="0.8"/>
      <SCALE sx="0.5" sy="0.5" sz="0.41"/>

      <DESCENDANTS>
        <LEAF type="cylinder" args="16 5 5 20 20 1 1"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="left-light">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

       <TRANSLATION x="18.5" y="5" z="6"/> 
       <ROTATION axis="y" angle="90"/>

      <DESCENDANTS>
        <LEAF type="cylinder" args="0.5 0.5 0.5 20 20 1 1"/> 
      </DESCENDANTS>
    </NODE>

    <NODE id="right-light">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

       <TRANSLATION x="18.5" y="5" z="2"/> 
       <ROTATION axis="y" angle="90"/>
       

      <DESCENDANTS>
        <LEAF type="cylinder" args="0.5 0.5 0.5 20 20 1 1"/> 
      </DESCENDANTS>
    </NODE>

    <NODE id="front-window">
      <MATERIAL id="separatorMaterial"/>
      <TEXTURE id="clear"/>


      <TRANSLATION x="18.8" y="9" z="0.5"/>
      

      <DESCENDANTS>
        <NODEREF id="top-window-separator"/>
        <NODEREF id="bot-window-separator"/>
        <NODEREF id="window-plate"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="top-window-separator">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0.025"/>
      <DESCENDANTS>
        <LEAF type="cylinder" args="6.95 0.25 0.25 20 20 1 1"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bot-window-separator">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>
      
      <TRANSLATION x="0" y="-3" z="0.025"/>
      <DESCENDANTS>
        <LEAF type="cylinder" args="6.95 0.25 0.25 20 20 1 1"/>
      </DESCENDANTS>

    </NODE>

     <NODE id="right-window-separator">
