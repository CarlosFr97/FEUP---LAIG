<SCENE>
  <!-- Inicial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
  </INITIALS>

  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
  </ILLUMINATION>

  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light0">
      <enable value ="1" />
      <position  x="0" y="12.5" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light1">
      <enable value ="1" />
      <position x="30" y="12.5" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light2">
      <enable value ="1" />
      <position x="0" y="12.5" z="20" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
      <enable value ="1" />
      <position x="30" y="12.5" z="20" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light4">
      <enable value ="1" />
      <position x="0" y="12.5" z="40" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light5">
      <enable value ="1" />
      <position x="30" y="12.5" z="40" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light6">
      <enable value ="1" />
      <position x="0" y="12.5" z="60" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

    <LIGHT id="light7">
      <enable value ="1" />
      <position x="30" y="12.5" z="60" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>
  </LIGHTS>

  <TEXTURES>
    <!-- Rocks -->
    <TEXTURE id="rocks">
      <file path="images/rocks.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Street bench -->
    <TEXTURE id="banco">
      <file path="images/bank.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- gravilha para chão comboio -->
    <TEXTURE id="gravilha">
      <file path="images/gravilha.jpg" />
      <amplif_factor s="0.15" t="0.15" />
    </TEXTURE>

    <!-- azulejo para paredes da estação -->
    <TEXTURE id="azulejo">
      <file path="images/azulejoPortuguesColorido.png" />
      <amplif_factor s="0.05" t="0.05" />
    </TEXTURE>

    <!-- madeira para as tabuas -->
    <TEXTURE id="madeiraCarris">
      <file path="images/madeira.png" />
      <amplif_factor s="0.5" t="0.5" />
    </TEXTURE>

    <!-- madeira para as tabuas -->
    <TEXTURE id="metalCarris">
      <file path="images/carris.png" />
      <amplif_factor s="0.5" t="0.5" />
    </TEXTURE>
  </TEXTURES>

  <MATERIALS>
    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- gravilha -->
    <MATERIAL id="materialGravilha">
      <shininess value="0.50" />
      <specular r="0.45" g="0.45" b="0.35" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="separatorMaterial">
      <shininess value="1"/>
      <specular r="0.1" g="0.1" b="0.1" a="1"/>
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="carriageMaterial">
      <shininess value="1"/>
      <specular r="0.807" g="0.807" b="0.0" a="1"/>
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.1" g="0.1" b="0.1" a="1" />
    </MATERIAL>

    <MATERIAL id="wheelMaterial">
      <shininess value="1"/>
      <specular r="0.268" g="0.274" b="0.294" a="1"/>
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.1" g="0.1" b="0.1" a="1" />
    </MATERIAL>

    <MATERIAL id="seatMaterial">
      <shininess value="1"/>
      <specular r="0.333" g="0.419" b="0.184" a="1"/>
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.1" g="0.1" b="0.1" a="1" />
    </MATERIAL>


  </MATERIALS>

  <LEAVES>
  </LEAVES>

  <NODES>
    <ROOT id="root"/>

    <!-- raiz -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="chaoCarris"/>
        <NODEREF id="laterias"/>
        <NODEREF id="paredes"/>
        <NODEREF id="train"/>
        <NODEREF id="carris"/>
        <NODEREF id="portasEstacao"/>
        <NODEREF id="wood-seat-left1"/>
        <NODEREF id="wood-seat-right1"/>
      </DESCENDANTS>
    </NODE>

    <!-- chão onde passam os carris -->
    <NODE id="chaoCarris">
      <MATERIAL id="materialGravilha"/>
      <TEXTURE id="gravilha"/>

      <SCALE sx="35" sy="0" sz="39.5" />
      <TRANSLATION x="0" y="0" z="1.5" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>
    </NODE>

    <!-- cais laterais -->
    <NODE id="laterias">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="lateralEsquerda"/>
        <NODEREF id="lateralDireita"/>
      </DESCENDANTS>
    </NODE>

    <!-- cais do lado esquerdo -->
    <NODE id="lateralEsquerda">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="70" sy="5" sz="15" />

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- cais do lado direito -->
    <NODE id="lateralDireita">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="70" sy="5" sz="15" />
      <TRANSLATION x="0" y="0" z="2.5"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- paredes -->
    <NODE id="paredes">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="azulejo"/>

      <DESCENDANTS>
        <NODEREF id="paredeEsquerda"/>
        <NODEREF id="paredeDireita"/>
      </DESCENDANTS>
    </NODE>

    <!-- parede esquerda -->
    <NODE id="paredeEsquerda">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="paredeGrande"/>
        <NODEREF id="paredeCimaPorta"/>
      </DESCENDANTS>
    </NODE>

    <!-- parede direita -->
    <NODE id="paredeDireita">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="59.8"/>

      <DESCENDANTS>
        <NODEREF id="paredeGrande"/>
        <NODEREF id="paredeCimaPorta"/>
      </DESCENDANTS>
    </NODE>

    <!-- parece grande -->
    <NODE id="paredeGrande">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="23.35" sy="22.5" sz="0.5" />
      <TRANSLATION x="0" y="0.11" z="0.5"/>
      <ROTATION axis="y" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- portas da estação -->
    <NODE id="portasEstacao">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="portaEstacaoEsquerda"/>
        <NODEREF id="portaEstacaoDireita"/>
      </DESCENDANTS>
    </NODE>

    <!-- portado lado esquerdo -->
    <NODE id="portaEstacaoEsquerda">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="22.5" y="0.32" z="0.5"/>

      <DESCENDANTS>
        <NODEREF id="portaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <!-- portado lado direita -->
    <NODE id="portaEstacaoDireita">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="8.4" y="0.32" z="59"/>

      <DESCENDANTS>
        <NODEREF id="portaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <!-- portado lado direita -->
    <NODE id="portaGenerica">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="rebordoPorta"/>
        <NODEREF id="paredePorta"/>
      </DESCENDANTS>
    </NODE>

    <!-- redordo da porta -->
    <NODE id="paredePorta">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="2.3" sy="13.6" sz="1"/>
      <TRANSLATION x="1.61" y="0.155" z="0"/>
      <ROTATION axis="y" angle="-90"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- redordo da porta -->
    <NODE id="rebordoPorta">
      <MATERIAL id="null"/>
      <TEXTURE id="madeiraCarris"/>

      <DESCENDANTS>
        <NODEREF id="rebordoEsquerda"/>
        <NODEREF id="rebordoDireita"/>
        <NODEREF id="rebordoCima"/>
      </DESCENDANTS>
    </NODE>

    <!-- rebordo da porta, parte do lado esquerdo -->
    <NODE id="rebordoEsquerda">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="0.25" sy="14" sz="1" />
      <TRANSLATION x="0" y="0.15" z="0.5"/>
      <ROTATION axis="y" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- rebordo da porta, parte do lado direito -->
    <NODE id="rebordoDireita">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="0.25" sy="14" sz="1" />
      <TRANSLATION x="15" y="0.15" z="0.5"/>
      <ROTATION axis="y" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- rebordo da porta, parte de cima -->
    <NODE id="rebordoCima">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="8.25" sy="0.25" sz="1" />
      <TRANSLATION x="0" y="37" z="0"/>
      <ROTATION axis="x" angle="-270"/>

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- CARRIS -->
    <NODE id="carris">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="carrilEsquerda"/>
        <NODEREF id="carrilDireita"/>
        <NODEREF id="tabuas"/>
      </DESCENDANTS>
    </NODE>

    <!-- carril esquerda -->
    <NODE id="carrilEsquerda">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0.25" z="26.75" />

      <DESCENDANTS>
        <NODEREF id="carrilGenerico"/>
      </DESCENDANTS>
    </NODE>

    <!-- carril direita -->
    <NODE id="carrilDireita">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0.25" z="32.25" />

      <DESCENDANTS>
        <NODEREF id="carrilGenerico"/>
      </DESCENDANTS>
    </NODE>

    <!-- carril genrico -->
    <NODE id="carrilGenerico">
      <MATERIAL id="null"/>
      <TEXTURE id="metalCarris"/>

      <SCALE sx="75" sy="1" sz="0.5" />

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- conjunto de tabuas -->
    <NODE id="tabuas">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="tabua1"/>
        <NODEREF id="tabua2"/>
        <NODEREF id="tabua3"/>
        <NODEREF id="tabua4"/>
        <NODEREF id="tabua5"/>
        <NODEREF id="tabua6"/>
        <NODEREF id="tabua7"/>
      </DESCENDANTS>
    </NODE>

    <!-- tabua 1 -->
    <NODE id="tabua1">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="1.5" y="0" z="26.1" />

      <DESCENDANTS>
        <NODEREF id="tabuaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <!-- tabua 2 -->
    <NODE id="tabua2">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="7" y="0" z="26.1" />

      <DESCENDANTS>
        <NODEREF id="tabuaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tabua3">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="12.5" y="0" z="26.1" />

      <DESCENDANTS>
        <NODEREF id="tabuaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tabua4">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="18" y="0" z="26.1" />

      <DESCENDANTS>
        <NODEREF id="tabuaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tabua5">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="23.5" y="0" z="26.1" />

      <DESCENDANTS>
        <NODEREF id="tabuaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tabua6">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="29" y="0" z="26.1" />

      <DESCENDANTS>
        <NODEREF id="tabuaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tabua7">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="34.5" y="0" z="26.1" />

      <DESCENDANTS>
        <NODEREF id="tabuaGenerica"/>
      </DESCENDANTS>
    </NODE>

    <!-- tabua genrica -->
    <NODE id="tabuaGenerica">
      <MATERIAL id="null"/>
      <TEXTURE id="madeiraCarris"/>

      <SCALE sx="2.5" sy="0.5" sz="5" />

      <DESCENDANTS>
        <NODEREF id="myRectangle"/>
      </DESCENDANTS>
    </NODE>

    <!-- antes -->
    <NODE id="train">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="18" y="-2.5" z="25.75"/>

      <DESCENDANTS>
        <NODEREF id="carriage_1"/>
        <NODEREF id="carriage_2"/>
        <NODEREF id="carriage_3"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="carriage_1">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="carriage"/>
        <NODEREF id="separator"/>
        <NODEREF id="top-light"/>
        <NODEREF id="left-light"/>
        <NODEREF id="right-light"/>
        <NODEREF id="front-window"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="carriage_2">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="-13" y="0" z="0" />

      <DESCENDANTS>
        <NODEREF id="carriage"/>
        <NODEREF id="separator"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="carriage_3">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="-26" y="0" z="0"/>

      <DESCENDANTS>
        <NODEREF id="carriage"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="carriage">
      <MATERIAL id="carriageMaterial"/>
      <TEXTURE id="null"/>

      <SCALE sx="2.5" sy="1" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="left-side"/>
        <NODEREF id="right-side"/>
        <NODEREF id="top-side"/>
        <NODEREF id="bottom-side"/>
        <NODEREF id="back-side"/>
        <NODEREF id="front-side"/>

        <NODEREF id="left-wheel1"/>
        <NODEREF id="left-wheel2"/>
        <NODEREF id="left-wheel3"/>
        <NODEREF id="right-wheel1"/>
        <NODEREF id="right-wheel2"/>
        <NODEREF id="right-wheel3"/>

        <NODEREF id="left-window"/>
        <NODEREF id="right-window"/>

        <NODEREF id="left-door"/>
        <NODEREF id="right-door"/>

        <NODEREF id="smooth-roof"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="left-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="2.5" y="4" z="7.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="right-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="7.5" y="4" z="0.5" />
      <ROTATION axis="y" angle="180" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="top-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="-90" />
      <TRANSLATION x="2.5" y="-7.5" z="11" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="bottom-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="2.5" y="4" z="0.5" />
      <ROTATION axis="x" angle="90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="front-side">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="7.5" y="4" z="7.5" />
		  <ROTATION axis="y" angle="90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 7 7 0 "/>
      </DESCENDANTS>
    </NODE>

    <NODE id="back-side">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="2.5" y="4" z="0.5" />
		  <ROTATION axis="y" angle="-90" />

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 7 7 0"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="separator">
      <MATERIAL id="separatorMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="5.2" y="0.49" z="0.5"/>
      <SCALE sx="0.15" sy="0.95" sz="0.85"/>

      <DESCENDANTS>
        <NODEREF id="left-side"/>
        <NODEREF id="top-side"/>
        <NODEREF id="right-side"/>
        <NODEREF id="bottom-side"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="top-light">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

       <TRANSLATION x="18.5" y="10" z="4"/>
       <ROTATION axis="y" angle="90"/>
       <SCALE sx="1" sy="0.5" sz="0.5"/>

      <DESCENDANTS>
        <LEAF type="cylinder" args="1 1 1 20 20 1 1"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="left-wheel1">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="6.5"/>

      <DESCENDANTS>
        <NODEREF id="wheel"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="left-wheel2">
